{
    "name": "peekr",
    "version": "1.0.5",
    "type": "module",
    "description": "Browser-based ONNX eye tracking module using MediaPipe + onnxruntime-web",
    "main": "dist/index.js",
    "module": "dist/index.js",
    "exports": {
        ".": {
            "import": "./dist/index.js",
            "require": "./dist/index.js"
        }
    },
    "files": [
        "dist",
        "model"
    ],
    "scripts": {
        "dev": "rollup --config --watch",
        "build": "rollup --config",
        "serve": "npx http-server dist",
        "lint": "npx eslint"
    },
    "keywords": [
        "eye-tracking",
        "onnx",
        "javascript",
        "webcam",
        "mediapipe",
        "onnxruntime-web"
    ],
    "author": "Aryaman Taore",
    "license": "MIT",
    "dependencies": {
        "@mediapipe/face_mesh": "^0.4.1633559619",
        "ndarray": "^1.0.19",
        "ndarray-ops": "^1.2.2",
        "onnxruntime-web": "1.19.2"
    },
    "devDependencies": {
        "@eslint/js": "^9.31.0",
        "@rollup/plugin-commonjs": "^25.0.0",
        "@rollup/plugin-node-resolve": "^15.2.3",
        "@rollup/plugin-terser": "^0.4.3",
        "@rollup/plugin-url": "^8.0.2",
        "@rollup/plugin-wasm": "^6.2.2",
        "@sentry/webpack-plugin": "^3.6.0",
        "@surma/rollup-plugin-off-main-thread": "^2.2.3",
        "copy-webpack-plugin": "^13.0.0",
        "dotenv": "^17.2.0",
        "eslint": "^9.31.0",
        "globals": "^16.3.0",
        "html-webpack-plugin": "^5.6.3",
        "ndarray": "^1.0.19",
        "ndarray-ops": "^1.2.2",
        "path-browserify": "^1.0.1",
        "rollup": "^4.0.2",
        "rollup-plugin-bundle-worker": "^0.1.0",
        "rollup-plugin-copy": "^3.5.0",
        "webpack": "^5.100.2",
        "webpack-cli": "^6.0.1",
        "webpack-dev-server": "^5.2.2",
        "webpack-merge": "^6.0.1"
    }
}
