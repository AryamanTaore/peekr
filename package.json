{
    "name": "peekr",
    "version": "1.0.5",
    "type": "module",
    "description": "Browser-based ONNX eye tracking module using MediaPipe + onnxruntime-web",
    "main": "dist/index.js",
    "module": "dist/index.js",
    "exports": {
        ".": {
            "import": "./dist/index.js",
            "require": "./dist/index.js"
        }
    },
    "files": [
        "dist",
        "model"
    ],
    "scripts": {
        "dev": "rollup --config --watch",
        "build": "rollup --config",
        "serve": "npx http-server dist",
        "lint": "npx eslint"
    },
    "keywords": [
        "eye-tracking",
        "onnx",
        "javascript",
        "webcam",
        "mediapipe",
        "onnxruntime-web"
    ],
    "author": "Aryaman Taore",
    "license": "MIT",
    "dependencies": {
        "ndarray": "^1.0.19",
        "ndarray-ops": "^1.2.2",
        "onnxruntime-web": "1.19.2"
    },
    "devDependencies": {
        "@eslint/js": "^9.31.0",
        "@rollup/plugin-commonjs": "^25.0.0",
        "@rollup/plugin-node-resolve": "^15.2.3",
        "@rollup/plugin-terser": "^0.4.3",
        "@rollup/plugin-url": "^8.0.2",
        "@rollup/plugin-wasm": "^6.2.2",
        "@surma/rollup-plugin-off-main-thread": "^2.2.3",
        "eslint": "^9.31.0",
        "ndarray": "^1.0.19",
        "ndarray-ops": "^1.2.2",
        "rollup": "^4.0.2",
        "rollup-plugin-bundle-worker": "^0.1.0",
        "rollup-plugin-copy": "^3.5.0"
    }
}
